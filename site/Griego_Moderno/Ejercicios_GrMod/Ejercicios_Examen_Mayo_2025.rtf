..\cf1\u964\'5f\u959\'5f\u965\'5f\cf0... ...\cf1\u964\'5f\u959\'5f\cf0... \u949\'5f\u964\'5f\u959\'5f\u953\'5f\u956\'5f\u940\'5f\u950\'5f\u949\'5f\u953\'5f ... ... ... ...\par\b 3\b0\'bc \u7977\'5f \u958\'5f\u945\'5f\u948\'5f\u941\'5f\u955\'5f\u966\'5f\u951\'5f \u956\'5f\u959\'5f\u965\'5f \u948\'5f\u940\'5f\u957\'5f\u949\'5f\u953\'5f\u963\'5f\u949\'5f \b\u963\'5f\b0' \b\u945\'5f\u965\'5f\u964\'5f\u959\'5f\u973\'5f\u962\'5f \ul\b0\u964\'5f\u953\'5f\u962\'5f\ulnone  \ul\u954\'5f\u945\'5f\u963\'5f\u941\'5f\u964\'5f\u949\'5f\u962\'5f\ulnone  \ul\u964\'5f\u951\'5f\u962\'5f\ulnone  \u8658\'5f \u7977\'5f \u958\'5f\u945\'5f\u948\'5f\u941\'5f\u955\'5f\u966\'5f\u951\'5f ...\cf1\u964\'5f\u959\'5f\u965\'5f\u962\'5f\cf0... ...\cf1\u964\'5f\u953\'5f\u962\'5f\cf0... \u948\'5f\u940\'5f\u957\'5f\u949\'5f\u953\'5f\u963\'5f\u949\'5f ... ... ... ...\par\b 4\b0\'bc \u904\'5f\u948\'5f\u969\'5f\u963\'5f\u945\'5f \b\u963\'5f\u964\'5f\u951\'5f \u956\'5f\u951\'5f\u964\'5f\u941\'5f\u961\'5f\u945\'5f \u956\'5f\u959\'5f\u965\'5f \ul\b0\u964\'5f\u951\'5f\u957\'5f\ulnone  \ul\u956\'5f\u960\'5f\u955\'5f\u959\'5f\u973\'5f\u950\'5f\u945\'5f\ulnone  \ul\u964\'5f\u951\'5f\u962\'5f\ulnone  \u8658\'5f ...\cf1\u964\'5f\u951\'5f\u962\'5f\cf0... ...\cf1\u964\'5f\u951\'5f\u957\'5f\cf0... \u941\'5f\u948\'5f\u969\'5f\u963\'5f\u945\'5f ... ... ... ...\par\b 5\b0\'bc \emdash  \u917\'5f\u943\'5f\u960\'5f\u945\'5f\u964\'5f\u949\'5f \b\u963\'5f\u964\'5f\b0' \b\u945\'5f\u948\'5f\u941\'5f\u955\'5f\u966\'5f\u953\'5f\u945\'5f \u956\'5f\u945\'5f\u962\'5f \ul\b0\u964\'5f\u945\'5f\ulnone  \ul\u957\'5f\u941\'5f\u945\'5f\ulnone; \emdash  \u908\'5f\u967\'5f\u953\'5f \u945\'5f\u954\'5f\u972\'5f\u956\'5f\u951\'5f. \u8658\'5f ...\cf1\u964\'5f\u959\'5f\u965\'5f\u962\'5f\cf0... ...\cf1\u964\'5f\u945\'5f\cf0... \u949\'5f\u943\'5f\u960\'5f\u945\'5f\u964\'5f\u949\'5f ... ... ... ... ; \emdash  \u908\'5f\u967\'5f\u953\'5f \u945\'5f\u954\'5f\u972\'5f\u956\'5f\u951\'5f.\par\par{\plain\cf0\up0\f0\b\fs24 6.\tab\f1\fs22 13\f0\fs24\u914\'5f}\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 13\b0\u914\'5f \u931\'5f\u965\'5f\u956\'5f\u960\'5f\u955\'5f\u942\'5f\u961\'5f\u969\'5f\u963\'5f\u949\'5f \u956\'5f\u949\'5f \u964\'5f\u959\'5f \u954\'5f\u945\'5f\u964\'5f\u945\'5f\u955\'5f\u955\'5f\u951\'5f\u955\'5f\u959\'5f \u961\'5f\u942\'5f\u956\'5f\u945\'5f\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx220\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 1\b0\'bc \u908\'5f\u964\'5f\u945\'5f\u957\'5f \u941\'5f\u967\'5f\u969\'5f \u963\'5f\u965\'5f\u957\'5f\u940\'5f\u967\'5f\u953\'5f ...\cf1\u966\'5f\u964\'5f\u949\'5f\u961\'5f\u957\'5f\u943\'5f\u950\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f\cf0... \b(\u966\'5f\u964\'5f\u949\'5f\u961\'5f\u957\'5f\u943\'5f\u950\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f)\par 2\b0\'bc \u922\'5f\u974\'5f\u963\'5f\u964\'5f\u945\'5f, ...\cf1\u941\'5f\u961\'5f\u967\'5f\u949\'5f\u963\'5f\u945\'5f\u953\'5f\cf0... \u956\'5f\u945\'5f\u950\'5f\u943\'5f \u956\'5f\u945\'5f\u962\'5f \u963\'5f\u964\'5f\u959\'5f \u963\'5f\u953\'5f\u957\'5f\u949\'5f\u956\'5f\u940\'5f; \b(\u941\'5f\u961\'5f\u967\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f)\par 3\b0\'bc \u932\'5f\u959\'5f \u957\'5f\u949\'5f\u961\'5f\u972\'5f ...\cf1\u947\'5f\u943\'5f\u957\'5f\u949\'5f\u964\'5f\u945\'5f\u953\'5f\cf0... \u960\'5f\u940\'5f\u947\'5f\u959\'5f\u962\'5f \u963\'5f\u964\'5f\u959\'5f\u965\'5f\u962\'5f \b 0\'bc (\u956\'5f\u951\'5f\u948\'5f\u941\'5f\u957\'5f \u946\'5f\u945\'5f\u952\'5f\u956\'5f\u959\'5f\u973\'5f\u962\'5f) (\u947\'5f\u943\'5f\u957\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f)\par 4\b0\'bc \u928\'5f\u945\'5f\u973\'5f\u955\'5f\u959\'5f, \u949\'5f\u956\'5f\u949\'5f\u943\'5f\u962\'5f \u963\'5f\u959\'5f\u965\'5f ...\cf1\u949\'5f\u965\'5f\u967\'5f\u972\'5f\u956\'5f\u945\'5f\u963\'5f\u964\'5f\u949\'5f\cf0... \u960\'5f\u949\'5f\u961\'5f\u945\'5f\u963\'5f\u964\'5f\u953\'5f\u954\'5f\u940\'5f \b(\u949\'5f\u973\'5f\u967\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f)\par 5\b0\'bc \u928\'5f\u959\'5f\u973\'5f ...\cf1\u954\'5f\u940\'5f\u952\'5f\u949\'5f\u963\'5f\u964\'5f\u949\'5f\cf0... \u949\'5f\u963\'5f\u949\'5f\u943\'5f\u962\'5f, \u960\'5f\u945\'5f\u953\'5f\u948\'5f\u953\'5f\u940\'5f; \b(\u954\'5f\u940\'5f\u952\'5f\u959\'5f\u956\'5f\u945\'5f\u953\'5f)\par\par{\plain\cf0\up0\f0\b\fs24 7.\tab\f1\fs22 14\f0\fs24\u913\'5f}\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1 14\b0\u913\'5f \u925\'5f\u945\'5f \u963\'5f\u965\'5f\u956\'5f\u960\'5f\u955\'5f\u951\'5f\u961\'5f\u974\'5f\u963\'5f\u949\'5f\u964\'5f\u949\'5f \u964\'5f\u953\'5f\u962\'5f \u954\'5f\u945\'5f\u964\'5f\u945\'5f\u955\'5f\u942\'5f\u958\'5f\u949\'5f\u953\'5f\u962\'5f\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx220\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 1\b0\'bc \u932\'5f\u959\'5f \u963\'5f\u960\'5f\u943\'5f\u964\'5f\u953\'5f \u956\'5f\u959\'5f\u965\'5f \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u963\'5f' \u941\'5f\u957\'5f\u945\'5f \u966\'5f\u945\'5f\u961\'5f\u948\'5f-\cf1\u973\'5f\cf0... \u954\'5f\u945\'5f\u953\'5f \u969\'5f\u961\'5f\u945\'5f\u943\'5f-\cf1\u959\'5f\cf0... \u948\'5f\u961\'5f\u972\'5f\u956\'5f\u959\'5f.\par\b 2\b0\'bc \emdash  \u915\'5f\u953\'5f\u945\'5f\u964\'5f\u943\'5f \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u963\'5f\u942\'5f\u956\'5f\u949\'5f\u961\'5f\u945\'5f \u951\'5f \u964\'5f\u963\'5f\u940\'5f\u957\'5f\u964\'5f\u945\'5f \u963\'5f\u959\'5f\u965\'5f \u949\'5f\u955\'5f\u945\'5f\u966\'5f\u961\'5f-\cf1\u953\'5f\u940\'5f\cf0... ; \emdash  \u915\'5f\u953\'5f\u945\'5f\u964\'5f\u943\'5f \u948\'5f\u949\'5f\u957\'5f \u941\'5f\u967\'5f\u949\'5f\u953\'5f \u946\'5f\u953\'5f\u946\'5f\u955\'5f\u943\'5f\u945\'5f.\par\b 3\b0\'bc \u919\'5f \u950\'5f\u974\'5f\u957\'5f\u951\'5f \u964\'5f\u959\'5f\u965\'5f \u952\'5f\u945\'5f\u955\'5f\u945\'5f\u963\'5f\u963\'5f-\cf1\u953\'5f\u959\'5f\u973\'5f\cf0... \u966\'5f\u959\'5f\u961\'5f\u941\'5f\u956\'5f\u945\'5f\u964\'5f\u972\'5f\u962\'5f \u956\'5f\u959\'5f\u965\'5f \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u960\'5f\u959\'5f\u955\'5f\u973\'5f \u966\'5f\u945\'5f\u961\'5f\u948\'5f-\cf1\u953\'5f\u940\'5f\cf0... .\par\b 4\b0\'bc \u904\'5f\u967\'5f\u949\'5f\u953\'5f \u948\'5f\u973\'5f\u959\'5f \u954\'5f\u945\'5f\u966\'5f\u949\'5f\u964\'5f-\cf1\u953\'5f\u959\'5f\u973\'5f\u962\'5f\cf0... \u963\'5f\u954\'5f\u973\'5f\u955\'5f\u959\'5f\u965\'5f\u962\'5f.\par\b 5\b0\'bc \u913\'5f\u965\'5f\u964\'5f\u972\'5f\u962\'5f \u959\'5f \u949\'5f\u965\'5f\u947\'5f\u949\'5f\u957\'5f\u953\'5f\u954\'5f\u972\'5f\u962\'5f \u954\'5f\u973\'5f\u961\'5f\u953\'5f\u959\'5f\u962\'5f \u941\'5f\u967\'5f\u949\'5f\u953\'5f \u941\'5f\u957\'5f\u945\'5f\u957\'5f \u960\'5f\u949\'5f\u953\'5f\u963\'5f\u956\'5f\u945\'5f\u964\'5f\u940\'5f\u961\'5f-\cf1\u951\'5f\cf0... \u954\'5f\u945\'5f\u953\'5f \u945\'5f\u947\'5f\u949\'5f\u957\'5f-\cf1\u942\'5f\cf0... \u947\'5f\u953\'5f\u959\'5f.\par\par{\plain\cf0\up0\f0\b\fs24 8.\tab\f1\fs22 14\f0\fs24\u914\'5f}\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 14\b0\u914\'5f \u925\'5f\u945\'5f \u947\'5f\u961\'5f\u940\'5f\u968\'5f\u949\'5f\u964\'5f\u949\'5f \u963\'5f\u964\'5f\u959\'5f\u957\'5f \u949\'5f\u957\'5f\u953\'5f\u954\'5f\u972\'5f \u964\'5f\u953\'5f\u962\'5f \u960\'5f\u945\'5f\u961\'5f\u945\'5f\u954\'5f\u940\'5f\u964\'5f\u969\'5f \u960\'5f\u961\'5f\u959\'5f\u964\'5f\u940\'5f\u963\'5f\u949\'5f\u953\'5f\u962\'5f\par\b 1\b0\'bc \b\u927\'5f\u953\'5f \u966\'5f\u943\'5f\u955\'5f\u949\'5f\u962\'5f \b0\u956\'5f\u959\'5f\u965\'5f \u948\'5f\u949\'5f\u957\'5f \b\u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u945\'5f\u957\'5f\u945\'5f\u953\'5f\u948\'5f\u949\'5f\u943\'5f\u962\'5f \b0\u954\'5f\u945\'5f\u953\'5f \b\u945\'5f\u947\'5f\u949\'5f\u957\'5f\u949\'5f\u943\'5f\u962\'5f\b0, \u972\'5f\u960\'5f\u969\'5f\u962\'5f \u955\'5f\u949\'5f\u962\'5f. \u8658\'5f ...\cf1\u919\'5f\cf0  \cf1\u966\'5f\u943\'5f\u955\'5f\u951\'5f\cf0... \u956\'5f\u959\'5f\u965\'5f \u948\'5f\u949\'5f\u957\'5f ...\cf1\u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f\cf0... ...\cf1\u945\'5f\u957\'5f\u945\'5f\u953\'5f\u948\'5f\u942\'5f\u962\'5f\cf0... \u954\'5f\u945\'5f\u953\'5f ...\cf1\u945\'5f\u947\'5f\u949\'5f\u957\'5f\u942\'5f\u962\'5f\cf0... , \u972\'5f\u960\'5f\u969\'5f\u962\'5f \u955\'5f\u949\'5f\u962\'5f.\par\b 2\b0\'bc \u924\'5f\u945\'5f\u962\'5f \b\u945\'5f\u961\'5f\u941\'5f\u963\'5f\u959\'5f\u965\'5f\u957\'5f \u945\'5f\u965\'5f\u964\'5f\u940\'5f \u964\'5f\u945\'5f \u966\'5f\u945\'5f\u961\'5f\u948\'5f\u953\'5f\u940\'5f \u960\'5f\u945\'5f\u957\'5f\u964\'5f\u949\'5f\u955\'5f\u972\'5f\u957\'5f\u953\'5f\u945\'5f\b0. \u8658\'5f \u924\'5f\u945\'5f\u962\'5f ...\cf1\u945\'5f\u961\'5f\u941\'5f\u963\'5f\u949\'5f\u953\'5f\cf0... ...\cf1\u945\'5f\u965\'5f\u964\'5f\u972\'5f\cf0... ...\cf1\u964\'5f\u959\'5f\cf0... ...\cf1\u966\'5f\u945\'5f\u961\'5f\u948\'5f\u973\'5f\cf0... ...\cf1\u960\'5f\u945\'5f\u957\'5f\u964\'5f\u949\'5f\u955\'5f\u972\'5f\u957\'5f\u953\'5f\cf0... .\par\b 3\b0\'bc \u902\'5f\u955\'5f\u955\'5f\u951\'5f \u966\'5f\u959\'5f\u961\'5f\u940\'5f, \b\u963\'5f\u945\'5f\u962\'5f \b0\u960\'5f\u945\'5f\u961\'5f\u945\'5f\u954\'5f\u945\'5f\u955\'5f\u974\'5f, \u957\'5f\u945\'5f \b\u949\'5f\u943\'5f\u963\'5f\u964\'5f\u949\'5f \u945\'5f\u954\'5f\u961\'5f\u953\'5f\u946\'5f\u949\'5f\u943\'5f\u962\'5f \b0\u963\'5f\u964\'5f\u945\'5f \u929\'5f\u945\'5f\u957\'5f\u964\'5f\u949\'5f\u946\'5f\u959\'5f\u973\'5f \b\u963\'5f\u945\'5f\u962\'5f\b0. \u8658\'5f \u902\'5f\u955\'5f\u955\'5f\u951\'5f \u966\'5f\u959\'5f\u961\'5f\u940\'5f, ...\cf1\u963\'5f\u949\'5f\cf0... \u960\'5f\u945\'5f\u961\'5f\u945\'5f\u954\'5f\u945\'5f\u955\'5f\u974\'5f, \u957\'5f\u945\'5f ...\cf1\u949\'5f\u943\'5f\u963\'5f\u945\'5f\u953\'5f\cf0... ...\cf1\u945\'5f\u954\'5f\u961\'5f\u953\'5f\u946\'5f\u942\'5f\u962\'5f\cf0... \u963\'5f\u964\'5f\u945\'5f \u929\'5f\u945\'5f\u957\'5f\u964\'5f\u949\'5f\u946\'5f\u959\'5f\u973\'5f ...\cf1\u963\'5f\u959\'5f\u965\'5f\cf0... .\par\b 4\b0\'bc \u916\'5f\u949\'5f\u957\'5f \b\u964\'5f\u959\'5f\u965\'5f\u962\'5f \u952\'5f\u941\'5f\u955\'5f\u959\'5f\u965\'5f\u956\'5f\u949\'5f \b0\u963\'5f\u964\'5f\u951\'5f\u957\'5f \u960\'5f\u945\'5f\u961\'5f\u941\'5f\u945\'5f \b\u956\'5f\u945\'5f\u962\'5f\b0, \u947\'5f\u953\'5f\u945\'5f\u964\'5f\u943\'5f \b\u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u947\'5f\u954\'5f\u961\'5f\u953\'5f\u957\'5f\u953\'5f\u940\'5f\u961\'5f\u951\'5f\u948\'5f\u949\'5f\u962\'5f\b0. \u8658\'5f \u916\'5f\u949\'5f\u957\'5f ...\cf1\u964\'5f\u959\'5f\u957\'5f\cf0... ...\cf1\u952\'5f\u941\'5f\u955\'5f\u969\'5f\cf0... \u963\'5f\u964\'5f\u951\'5f\u957\'5f \u960\'5f\u945\'5f\u961\'5f\u941\'5f\u945\'5f ...\cf1\u956\'5f\u959\'5f\u965\'5f\cf0... , \u947\'5f\u953\'5f\u945\'5f\u964\'5f\u943\'5f ...\cf1\u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f\cf0... ...\cf1\u947\'5f\u954\'5f\u961\'5f\u953\'5f\u957\'5f\u953\'5f\u940\'5f\u961\'5f\u951\'5f\u962\'5f\cf0... .\par\b 5\b0\'bc \b\u913\'5f\u965\'5f\u964\'5f\u959\'5f\u943\'5f \u959\'5f\u953\'5f \u951\'5f\u952\'5f\u959\'5f\u960\'5f\u959\'5f\u953\'5f\u959\'5f\u943\'5f \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \b0\u960\'5f\u959\'5f\u955\'5f\u973\'5f \b\u948\'5f\u951\'5f\u956\'5f\u959\'5f\u966\'5f\u953\'5f\u955\'5f\u949\'5f\u943\'5f\u962\'5f \b0\u963\'5f\u964\'5f\u951\'5f\u957\'5f \u960\'5f\u945\'5f\u964\'5f\u961\'5f\u943\'5f\u948\'5f\u945\'5f \b\u964\'5f\u959\'5f\u965\'5f\u962\'5f \b0\u8658\'5f ...\cf1\u913\'5f\u965\'5f\u964\'5f\u959\'5f\u962\'5f\cf0... ...\cf1\u959\'5f\cf0... ...\cf1\u951\'5f\u952\'5f\u959\'5f\u960\'5f\u959\'5f\u953\'5f\u972\'5f\u962\'5f\cf0... \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u960\'5f\u959\'5f\u955\'5f\u973\'5f ...\cf1\u948\'5f\u951\'5f\u956\'5f\u959\'5f\u966\'5f\u953\'5f\u955\'5f\u942\'5f\u962\'5f\cf0... \u963\'5f\u964\'5f\u951\'5f\u957\'5f \u960\'5f\u945\'5f\u964\'5f\u961\'5f\u943\'5f\u948\'5f\u945\'5f ...\cf1\u964\'5f\u959\'5