5f \u946\'5f\u955\'5f\u945\'5f\u946\'5f\u949\'5f\u961\'5f\u972\'5f ...\cf1\u957\'5f\u945\'5f\cf0... ...\cf1\u964\'5f\u961\'5f\u974\'5f\u949\'5f\u953\'5f\cf0... \u954\'5f\u945\'5f\u957\'5f\u949\'5f\u943\'5f\u962\'5f \u954\'5f\u961\'5f\u941\'5f\u945\'5f\u962\'5f \u954\'5f\u940\'5f\u952\'5f\u949\'5f \u956\'5f\u941\'5f\u961\'5f\u945\'5f. \b(\u964\'5f\u961\'5f\u974\'5f\u969\'5f)\par 3\b0\'bc \u913\'5f\u954\'5f\u972\'5f\u956\'5f\u945\'5f ...\cf1\u957\'5f\u945\'5f\cf0... ...\cf1\u947\'5f\u965\'5f\u961\'5f\u943\'5f\u963\'5f\u949\'5f\u953\'5f\cf0... \u951\'5f \u913\'5f\u955\'5f\u949\'5f\u958\'5f\u940\'5f\u957\'5f\u948\'5f\u961\'5f\u945\'5f; \b(\u947\'5f\u965\'5f\u961\'5f\u943\'5f\u950\'5f\u969\'5f)\par 4\b0\'bc \u931\'5f\u965\'5f\u957\'5f\u951\'5f\u952\'5f\u943\'5f\u950\'5f\u949\'5f\u953\'5f ...\cf1\u957\'5f\u945\'5f\cf0... ...\cf1\u961\'5f\u943\'5f\u967\'5f\u957\'5f\u949\'5f\u953\'5f\cf0... \u957\'5f\u949\'5f\u961\'5f\u972\'5f \u963\'5f\u964\'5f\u945\'5f \u955\'5f\u959\'5f\u965\'5f\u955\'5f\u959\'5f\u973\'5f\u948\'5f\u953\'5f\u945\'5f \u954\'5f\u940\'5f\u952\'5f\u949\'5f \u945\'5f\u960\'5f\u972\'5f\u947\'5f\u949\'5f\u965\'5f\u956\'5f\u945\'5f. \b(\u961\'5f\u943\'5f\u967\'5f\u957\'5f\u969\'5f)\par 5\b0\'bc \u920\'5f\u941\'5f\u955\'5f\u959\'5f\u965\'5f\u957\'5f ...\cf1\u957\'5f\u945\'5f\cf0... ...\cf1\u949\'5f\u960\'5f\u953\'5f\u963\'5f\u964\'5f\u961\'5f\u941\'5f\u968\'5f\u959\'5f\u965\'5f\u957\'5f\cf0... \u963\'5f\u964\'5f\u951\'5f\u957\'5f \u917\'5f\u955\'5f\u955\'5f\u940\'5f\u948\'5f\u945\'5f. \b(\u949\'5f\u960\'5f\u953\'5f\u963\'5f\u964\'5f\u961\'5f\u941\'5f\u966\'5f\u974\'5f)\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx220\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\par\par{\plain\cf0\up0\f0\b\fs24 18.\tab\f1\fs22 16\f0\fs24\u917\'5f}\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1 16\u917\'5f \u925\'5f\u945\'5f \u963\'5f\u965\'5f\u956\'5f\u960\'5f\u955\'5f\u951\'5f\u961\'5f\u974\'5f\u963\'5f\u949\'5f\u964\'5f\u949\'5f \u964\'5f\u945\'5f \u954\'5f\u949\'5f\u957\'5f\u940\'5f \u956\'5f\u949\'5f\u964\'5f\u945\'5f \u960\'5f\u945\'5f\u961\'5f\u945\'5f\u954\'5f\u940\'5f\u964\'5f\u969\'5f \u961\'5f\u942\'5f\u956\'5f\u945\'5f\u964\'5f\u945\'5f \u963\'5f\u964\'5f\u959\'5f \u963\'5f\u969\'5f\u963\'5f\u964\'5f\u972\'5f \u964\'5f\u973\'5f\u960\'5f\u959\'5f\b0: \b\u956\'5f\u945\'5f\u947\'5f\u949\'5f\u953\'5f\u961\'5f\u949\'5f\u973\'5f\u969\'5f\b0, \b\u948\'5f\u953\'5f\u945\'5f\u963\'5f\u954\'5f\u949\'5f\u948\'5f\u940\'5f\u950\'5f\u969\'5f\b0, \b\u948\'5f\u953\'5f\u974\'5f\u967\'5f\u957\'5f\u969\'5f\b0, \b\u968\'5f\u940\'5f\u967\'5f\u957\'5f\u969\'5f\b0, \b\u964\'5f\u945\'5f\u958\'5f\u953\'5f\u948\'5f\u949\'5f\u973\'5f\u969\'5f\b0, \b\u967\'5f\u959\'5f\u961\'5f\u949\'5f\u973\'5f\u969\'5f\b0, \b\u945\'5f\u955\'5f\u955\'5f\u940\'5f\u950\'5f\u969\'5f\b0, \b\u945\'5f\u957\'5f\u959\'5f\u943\'5f\u947\'5f\u969\'5f\b0, \b\u948\'5f\u953\'5f\u945\'5f\u955\'5f\u941\'5f\u947\'5f\u969\'5f\b0, \b\u948\'5f\u953\'5f\u945\'5f\u946\'5f\u940\'5f\u950\'5f\u969\'5f\b0, \b\u956\'5f\u960\'5f\u945\'5f\u943\'5f\u957\'5f\u969\'5f\b0, \b\u964\'5f\u951\'5f\u955\'5f\u949\'5f\u966\'5f\u969\'5f\u957\'5f\u974\'5f\b0.\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx220\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 1\b0\'bc \u917\'5f\u957\'5f\u974\'5f \u964\'5f\u945\'5f \u960\'5f\u945\'5f\u953\'5f\u948\'5f\u953\'5f\u940\'5f ...\cf1\u948\'5f\u953\'5f\u945\'5f\u963\'5f\u954\'5f\u941\'5f\u948\'5f\u945\'5f\u950\'5f\u945\'5f\u957\'5f\cf0... , \u958\'5f\u945\'5f\u966\'5f\u957\'5f\u953\'5f\u954\'5f\u940\'5f ...\cf1\u940\'5f\u957\'5f\u959\'5f\u953\'5f\u958\'5f\u949\'5f\cf0... \u951\'5f \u960\'5f\u972\'5f\u961\'5f\u964\'5f\u945\'5f \u954\'5f\u945\'5f\u953\'5f ...\cf1\u956\'5f\u960\'5f\u942\'5f\u954\'5f\u949\'5f\cf0... \u941\'5f\u957\'5f\u945\'5f\u962\'5f \u966\'5f\u943\'5f\u955\'5f\u959\'5f\u962\'5f \u964\'5f\u959\'5f\u965\'5f\u962\'5f \u947\'5f\u949\'5f\u956\'5f\u940\'5f\u964\'5f\u959\'5f\u962\'5f \u945\'5f\u943\'5f\u956\'5f\u945\'5f\u964\'5f\u945\'5f.\par\b 2\b0\'bc \emdash  ...\cf1\u935\'5f\u972\'5f\u961\'5f\u949\'5f\u968\'5f\u949\'5f\u962\'5f\cf0... \u963\'5f\u964\'5f\u959\'5f \u967\'5f\u952\'5f\u949\'5f\u963\'5f\u953\'5f\u957\'5f\u972\'5f \u960\'5f\u940\'5f\u961\'5f\u964\'5f\u953\'5f, \u915\'5f\u953\'5f\u974\'5f\u961\'5f\u947\'5f\u959\'5f; \emdash  \u925\'5f\u945\'5f\u953\'5f, \u960\'5f\u959\'5f\u955\'5f\u973\'5f.\par\b 3\b0\'bc \u928\'5f\u945\'5f\u953\'5f\u948\'5f\u953\'5f\u940\'5f, \u960\'5f\u961\'5f\u941\'5f\u960\'5f\u949\'5f\u953\'5f ...\cf1\u957\'5f\u945\'5f\cf0... ...\cf1\u948\'5f\u953\'5f\u945\'5f\u946\'5f\u940\'5f\u963\'5f\u949\'5f\u964\'5f\u949\'5f\cf0... \u960\'5f\u959\'5f\u955\'5f\u973\'5f, \u947\'5f\u953\'5f\u945\'5f\u964\'5f\u943\'5f \u959\'5f\u953\'5f \u949\'5f\u958\'5f\u949\'5f\u964\'5f\u940\'5f\u963\'5f\u949\'5f\u953\'5f\u962\'5f \u960\'5f\u955\'5f\u951\'5f\u963\'5f\u953\'5f\u940\'5f\u950\'5f\u959\'5f\u965\'5f\u957\'5f.\par\b 4\b0\'bc \emdash  \u922\'5f\u940\'5f\u952\'5f\u949\'5f \u960\'5f\u972\'5f\u964\'5f\u949\'5f ...\cf1\u964\'5f\u951\'5f\u955\'5f\u949\'5f\u966\'5f\u969\'5f\u957\'5f\u949\'5f\u943\'5f\u964\'5f\u949\'5f\cf0... \u963\'5f\u964\'5f\u959\'5f\u965\'5f\u962\'5f \u947\'5f\u959\'5f\u957\'5f\u949\'5f\u943\'5f\u962\'5f \u963\'5f\u945\'5f\u962\'5f; \emdash  \u924\'5f\u953\'5f\u945\'5f \u966\'5f\u959\'5f\u961\'5f\u940\'5f \u963\'5f\u964\'5f\u953\'5f\u962\'5f \u948\'5f\u949\'5f\u954\'5f\u945\'5f\u960\'5f\u941\'5f\u957\'5f\u964\'5f\u949\'5f.\par\b 5\b0\'bc \u908\'5f\u964\'5f\u945\'5f\u957\'5f \u942\'5f\u956\'5f\u945\'5f\u963\'5f\u964\'5f\u945\'5f\u957\'5f \u966\'5f\u959\'5f\u953\'5f\u964\'5f\u951\'5f\u964\'5f\u941\'5f\u962\'5f, ...\cf1\u964\'5f\u945\'5f\u958\'5f\u953\'5f\u948\'5f\u949\'5f\u973\'5f\u945\'5f\u956\'5f\u949\'5f\cf0... \u960\'5f\u959\'5f\u955\'5f\u973\'5f \u964\'5f\u953\'5f\u962\'5f \u948\'5f\u953\'5f\u945\'5f\u954\'5f\u959\'5f\u960\'5f\u941\'5f\u962\'5f.\par\par{\plain\cf0\up0\f0\b\fs24 19.\tab\f1\fs22 17\f0\fs24\u913\'5f}\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 17\u913\'5f \b0\u925\'5f\u945\'5f \u963\'5f\u965\'5f\u956\'5f\u960\'5f\u955\'5f\u951\'5f\u961\'5f\u974\'5f\u963\'5f\u949\'5f\u964\'5f\u949\'5f \u964\'5f\u945\'5f \u954\'5f\u949\'5f\u957\'5f\u940\'5f \u956\'5f\u949\'5f \u964\'5f\u951\'5f\u957\'5f \u960\'5f\u961\'5f\u959\'5f\u963\'5f\u964\'5f\u945\'5f\u954\'5f\u964\'5f\u953\'5f\u954\'5f\u942\'5f \b(\u963\'5f\u965\'5f\u957\'5f\u949\'5f\u967\'5f\u942\'5f \u942\'5f \u945\'5f\u960\'5f\u955\'5f\u942\'5f) \b0\u964\'5f\u969\'5f\u957\'5f \u961\'5f\u951\'5f\u956\'5f\u940\'5f\u964\'5f\u969\'5f\u957\'5f \u960\'5f\u959\'5f\u965\'5f \u949\'5f\u943\'5f\u957\'5f\u945\'5f\u953\'5f \u963\'5f\u964\'5f\u951\'5f\u957\'5f \u960\'5f\u945\'5f\u961\'5f\u941\'5f\u957\'5f\u952\'5f\u949\'5f\u963\'5f\u951\'5f completad los huecos con el imperativo \b(continuo o simple) \b0de los verbos entre par\'8entesis\cf2\par\cf0\par\pard\ltrpar\ql\fi0\li0\ri0\lin0\rin0\tx220\tx720\tx1440\tx2160\tx2880\tx3600\tx4320\tx5040\tx5760\tx6480\tx7200\tx7920\sl360\slmult1\b 1\b0\'bc \u924\'5f\u945\'5f\u961\'5f\u943\'5f\u945\'5f, ...\cf1\u940\'5f\u955\'5f\u955\'5f\u945\'5f\u950\'5f\u949\'5f\cf0... \u964\'5f\u945\'5f \u963\'5f\u949\'5f\u957\'5f\u964\'5f\u972\'5f\u957\'5f\u953\'5f\u945\'5f \u963\'5f\u959\'5f\u965\'5f \u963\'5f\u965\'5f\u967\'5f\u957\'5f\u940\'5f. \b(\u945\'5f\u955\'5f\u955\'5f\u940\'5f\u950\'5f\u969\'5f)\par 2\b0\'bc \u928\'5f\u945\'5f\u953\'5f\u948\'5f\u953\'5f\u940\'5f, ...\cf1\u963\'5f\u964\'5f\u949\'5f\u943\'5f\u955\'5f\u949\'5f\u964\'5f\u949\'5f\cf0/\cf1\u963\'5f\u964\'5f\u949\'5f\u943\'5f\u955\'5f\u964\'5f\u949\'5f\cf0... \u964\'5f\u945\'5f \u967\'5f\u961\'5f\u942\'5f\u956\'5f\u945\'5f\u964\'5f\u945\'5f \u956\'5f\u949\'5f \u964\'5f\u959\'5f \u964\'5f\u945\'5f\u967\'5f\u965\'5f\u948\'5f\u961\'5f\u959\'5f\u956\'5f\u949\'5f\u943\'5f\u959\'5